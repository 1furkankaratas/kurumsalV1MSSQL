<div class="wrapper dark-wrapper inverse-text">
    <div class="container inner">
        <div class="row">
            <div class="col-md-8 offset-md-2">
                <h2 class="title-color color-default mb-20 text-center">İletişim</h2>
                <hr class="double w-25 mx-auto p-0">
                <div class="space40"></div>
                <ul class="icon-list list-inline text-center">
                    <li class="list-inline-item"><i class="fa fa-map"></i>@Model.Address</li>
                    <li class="list-inline-item"><i class="fa fa-envelope"></i><a href="mailto:@Model.Email" class="nocolor"></a>@Model.Email</li>
                    <li class="list-inline-item"><i class="fa fa-phone-square"></i>@Model.Phone </li>
                    <li class="list-inline-item"><i class="fa fa-clock-o"></i>@Model.WorkTime </li>
                </ul>
            </div>
        </div>
        <div class="space30"></div>
        <div class="row gutter-40">
            <div class="col-md-12">
                <iframe src="@Model.Maps"
                        width="100%" height="600" frameborder="0" style="border:0" allowfullscreen=""></iframe>
            </div>
        </div>

    </div>
</div>